<%- include("partials/header") -%> 
<h1>Home</h1>
<p><%= homeContent %></p>

<% posts.forEach(post => { %> 
   <h1><%= post.title %></h1>
   <div id="home-message">
    <p><%= post.message.substring(0, 100) + "..." %></p> 
    <a href="<%= "posts/" + post.title %>">Read more</a>
   </div>

<% }); %>
    
<%- include("partials/footer") -%> 